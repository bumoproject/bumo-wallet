{
  "name": "bumo-wallet",
  "version": "1.3.3",
  "description": "bumo wallet",
  "productName": "BumoWallet",
  "scripts": {
    "build": "node tasks/release.js",
    "build:clean": "cross-env PLATFORM_TARGET=clean node tasks/release.js",
    "build:darwin": "cross-env PLATFORM_TARGET=darwin node tasks/release.js",
    "build:darwin-Lite": "cross-env PLATFORM_TARGET=darwin NETWORK_TYPE=Lite node tasks/release.js",
    "build:darwin-test": "cross-env PLATFORM_TARGET=darwin NETWORK_TYPE=test node tasks/release.js",
    "build:linux": "cross-env PLATFORM_TARGET=linux node tasks/release.js",
    "build:mas": "cross-env PLATFORM_TARGET=mas node tasks/release.js",
    "build:win32": "cross-env PLATFORM_TARGET=win32 node tasks/release.js",
    "build:win32-Lite": "cross-env PLATFORM_TARGET=win32 NETWORK_TYPE=Lite node tasks/release.js",
    "build:win32-test": "cross-env PLATFORM_TARGET=win32 NETWORK_TYPE=test node tasks/release.js",
    "dev": "node tasks/runner.js",
    "dev-test": "cross-env NETWORK_TYPE=test node tasks/runner.js",
    "dev-Lite": "cross-env NETWORK_TYPE=Lite node tasks/runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack-Lite": "npm run pack:main-Lite && npm run pack:renderer-Lite",
    "pack-test": "npm run pack:main-test && npm run pack:renderer-test",
    "pack:main": "cross-env NODE_ENV=production webpack -p --progress --colors --config webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=renderer webpack -p --progress --colors --config webpack.renderer.config.js",
    "pack:main-Lite": "cross-env NODE_ENV=production NETWORK_TYPE=Lite webpack -p --progress --colors --config webpack.main.config.js",
    "pack:main-test": "cross-env NODE_ENV=production NETWORK_TYPE=test webpack -p --progress --colors --config webpack.main.config.js",
    "pack:renderer-Lite": "cross-env NODE_ENV=renderer NETWORK_TYPE=Lite webpack -p --progress --colors --config webpack.renderer.config.js",
    "pack:renderer-test": "cross-env NODE_ENV=renderer NETWORK_TYPE=test webpack -p --progress --colors --config webpack.renderer.config.js",
    "postinstall": "npm run lint:fix && cd app && npm install",
    "installer-mac": "electron-installer-dmg ./build/BumoWallet-darwin-x64/BumoWallet.app BumoWallet-gui --out=./build --background=app/icons/setup-bg.png --icon=app/icons/logo.icns --overwrite --debug"
  },
  "author": "bumo",
  "license": "",
